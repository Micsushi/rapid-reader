{
  "name": "Boldify",
  "version": "1.0",
  "description": "Makes every word on a page bold.",
  "permissions": ["activeTab"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
